{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "ajcastro/eager-load-pivot-relations": "^0.3.0",
        "awobaz/compoships": "^2.1",
        "aws/aws-sdk-php": "^3.347",
        "bavix/laravel-wallet": "^9.6",
        "botman/botman": "^2.8",
        "botman/driver-web": "^1.5",
        "google/apiclient": "^2.18",
        "google/apiclient-services": "*",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^1.3",
        "kreait/laravel-firebase": "*",
        "laravel/cashier": "^14.8",
        "laravel/framework": "^10.0",
        "laravel/passport": "^11.0",
        "laravel/sanctum": "^3.2",
        "laravel/socialite": "^5.5",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.29",
        "maatwebsite/excel": "^3.1",
        "midtrans/midtrans-php": "^2.5",
        "minuteoflaravel/laravel-audio-video-validator": "^1.1",
        "mollie/laravel-mollie": "^2.14",
        "orangehill/iseed": "^3.0",
        "predis/predis": "^3.0",
        "pusher/pusher-php-server": "^7.2",
        "pusher/pusher-push-notifications": "^2.0",
        "shift4/shift4-php": "^3.1",
        "spatie/laravel-translatable": "^6.3",
        "srmklive/paypal": "~3.0",
        "tightenco/ziggy": "^1.5",
        "tutorhub/laravel-installer": "dev-main"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "authorizenet/authorizenet": "*",
        "itsgoingd/clockwork": "^5.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "repositories": [
        {
          "type": "path",
          "url": "./lib/*"
        }
      ],
    "autoload": {
         "files": [
            "app/Utils/Helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform-check": false
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
